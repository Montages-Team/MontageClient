{
  "name": "MontageClient",
  "version": "1.0.1",
  "description": "Minimal web app",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./index.html",
    "build": "parcel build index.html --public-url ./",
    "wipe": "rm -rf .git && git init && git add . && git commit -m 'initial commit'",
    "deploy": "npm run build && ./scripts/gh-pages.sh",
    "test": "jest --coverage --no-cache",
    "prettier": "prettier --write --single-quote --semi=false --trailing-comma es5 \"src/app.js\""
  },
  "author": "Regis Boudinot (selfup)",
  "license": "MIT",
  "dependencies": {
    "hyperapp": "^1.2.5",
    "hyperapp-html": "^2.0.0",
    "materialize-css": "^0.100.2",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "@hyperapp/fx": "^0.7.0",
    "@hyperapp/html": "^1.1.1",
    "@hyperapp/logger": "^0.5.0",
    "@hyperapp/render": "^2.0.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-react": "^7.5.1",
    "imports": "^1.0.0",
    "jest": "^22.1.4",
    "node-libs-browser": "^2.0.0",
    "parcel-bundler": "^1.8.1",
    "parcel-plugin-eslint": "^1.0.4",
    "typescript": "^2.9.1"
  }
}
