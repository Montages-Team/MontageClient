adius: \"5px\",\n    boxShadow: \"1px 1px 2px #ddd\",\n    \"@media (max-width: 770px)\": {\n      display: \"block\",\n      float: \"none\",\n      position: \"relative\",\n      width: \"100%\",\n    },\n  })\n  return (\n    <Wrapper>\n      <ContentWrapper class=\"clearfix\">\n        <LeftColumn>\n          <img id=\"profile\" src=\"http://kalamu.com/neogriot/wp-content/uploads/2016/11/prince-02.jpg\"/>\n          <ProfileArea>\n            <UserName>Prince</UserName>\n            <p>@mynameisprince</p>\n            Follow: <a href=\"#\">10</a> Follower: <a href=\"#\">10</a>\n          </ProfileArea>\n        </LeftColumn>\n        <RightColumn>\n          <Heading class=\"mb15\">Princeに質問する</Heading>\n          <Questions>\n            hoge\n          </Questions>\n        </RightColumn>\n      </ContentWrapper>\n    </Wrapper>\n  )\n}"},"lineCount":null}},"hash":"22d8f072ddd316a9d72a30ce32e123c2","cacheData":{"env":{}}}